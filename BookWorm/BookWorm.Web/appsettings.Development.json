{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityConfiguration": {
    "Password": {
      "RequireUppercase": false,
      "RequireLowercase": false,
      "RequireNonAlphanumerical": false,
      "RequiredLength": 6
    },
    "SignIn": {
      "RequireConfirmedAccount":  false
    }
  }
}
